	a
		display block
		font-size 16px
		color #000
		line-height 22px
		text-decoration none
		padding 26px 0
		border-bottom 1px solid #E2E2E2
		transition color 0.2s

		&:hover
			color #2482C5

		&:last-child
			border-bottom 0

	& p

		&:after
			display block
			content ''
			width 100%
			height 1px
			margin 15px auto
			background #e2e2e2

		&:last-child

			&:after
				display none

.detail__head
	font-size 20px
	font-weight 600
	display inline-block
	padding-bottom 12px

	&:after
		display block
		content ''
		width 100%
		height 3px
		margin 5px 0 15px
		background #2482c5

/*DESCRIPTION PAGE. BLOCK 1 DESCRIPTION*/
.section
	width 100%

	&.-description
		-webkit-box-sizing border-box
		-moz-box-sizing border-box
		box-sizing border-box
