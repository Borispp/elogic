.popup
	position fixed
	z-index 9999
	left 50%
	top 50%
	transform translate(-50%, -50%)
	background #fff
	font-family "Open Sans"
	color #000
	width 510px
	-webkit-border-radius 10px
	-moz-border-radius 10px
	border-radius 10px
	padding 40px
	box-shadow 0 5px 10px rgba(0,0,0,0.2)
	box-sizing border-box
	will-change transform
	transition transform 0.4s

	.success
		text-align center
		line-height 36px
		padding-bottom 24px
		padding-top 36px
		font-size 24px
		font-weight 600
		letter-spacing -1px

	.calltoapel
		&.-hide
			display none

	&.-hide
		display block
		transform translate(-50%, -250%)

	.close
		display block
		position absolute
		top 24px
		right 24px
		background url('../images/close.png')
		width 20px
		height 20px
		opacity 0.4
		transition opacity 0.2s

		&:hover
			opacity 1

	& span
		display block
		text-align center
		color #4a4a4a
		font-size 15px
		lone-height 10px

