*
	outline none

body
	background #eee
	color #fff
	font-family 'Open Sans'

.wrapper
	width 100%
	position relative
	max-width 1440px
	min-width 1260px
	margin 0 auto
	box-shadow 0 0 6px rgba(0, 0, 0, 0.5)
	overflow hidden

.base-col
	width 100%
	max-width 1200px
	min-width 1000px
	margin 0 auto
	position relative

/*INDEX PAGE. Header*/
.header
	width 100%
	position absolute
	z-index 1
	padding-top 30px

	&.-white

		& a
			color #000
			transition color 0.2s

			&:hover
				color #51C423

.header__logo
	width 212px
	height 65px
	float left
	margin-left 0px

.header__nav
	float right
	font-family 'Open Sans'
	margin-top 18px
	position relative
	right 10px

	li
		display inline-block
		margin-right 30px

		a
			text-decoration none
			color #fff
			font-family "Open Sans"
			font-size 18px
			opacity 1
			transition opacity 0.2s


		&:last-child
			margin-right 0

		&:hover
			opacity 0.85

		&.-white
			color #000

		&:last-child
			font-size 22px
			font-weight 600

/*BLOCK 1*/
.section
	width 100%

	&.-cleaning_system
		position relative
		color #fff
		background url('../images/bg1.png') no-repeat
		padding 120px 0 340px
		text-align center

.headline-1
	text-align center
	font-family "Open Sans"
	font-weight 600
	font-size 62px
	line-height 70px
	margin-top 90px

	span
		display block
		margin 0 auto
		letter-spacing 1px
		line-height 30px
		max-width 400px
		font-size 18px
		font-weight 400

.h1_var2
	margin-top 60px

.advantages-bg
	width 100%
	position absolute
	// top 120px
	bottom 0
	background rgba(91, 194, 35, 0.84)

.clean_syst__advantages
	font-size 0

.advantages-col
	display inline-block
	padding 54px 0 28px
	width 25%
	margin-left 0
	font-family "Open Sans"
	font-weight 600
	font-size 16px
	text-align center
	vertical-align top

	&:before
		margin-left 0
		position relative
		left 50%
		transform translate(-50%, 0)

	span
		font-family "Montserrat"
		font-weight 400
		font-size 45px
		line-height 80px

	& p
		line-height 24px
		font-size 16px
		font-weight 400

	.icon
		display block
		margin 0 auto
		text-align center

/*BLOCK 2*/
.section
	width 100%

	&.-ecologic_about
		min-height 945px
		background #fff1b3 url('../images/eco.png') no-repeat
		font-family "Open Sans"
		text-align center

		.headline-2
			span
				color #fff

.headline-2
	font-weight 700
	font-family 'Open Sans'
	font-size 30px
	line-height 30px
	padding 35px 0

	&.-black
		color #000

	& span
		font-size 12px
		color #4A4A4A
		letter-spacing 5px

.eco_about__text
	font-family 'Open Sans'
	font-size 20px
	color #FFF
	line-height 28px
	font-weight 300

/*BLOCK 3*/
.section
	width 100%

	&.-more_information
		background #fff1b3
		text-align center
		font-family "Open Sans"

.eco_about__realize
	padding 35px 0

.realization_process
	font-size 0
	padding-bottom 60px

	.icon
		display block
		margin 0 auto

.realize-col
	display inline-block
	width 25%
	margin-left 0
	padding 25px 0
	color #000
	vertical-align top

	&:before
		margin-left 0
		position relative
		left 50%
		transform translate(-50%, 0)

	& span
		font-weight 400
		font-size 22px
		line-height 65px

	& p
		font-family 'Open Sans'
		font-size 14px
		color #787878
		line-height 22px
		font-weight 300
		margin 0 20px

	&.-questionnaire

		&:before
			display block
			width 129px
			height 129px
			content ''
			background url('../images/cheste.png')

	&.-size

		&:before
			display block
			width 129px
			height 128px
			content ''
			background url('../images/mas.png')

	&.-eco-install

		&:before
			display block
			width 128px
			height 128px
			content ''
			background url('../images/installation.png')

	&.-eco-advant

		&:before
			display block
			width 128px
			height 128px
			content ''
			background url('../images/advantages.png')

/*BLOCK 4*/
.section
	width 100%

	&.-custom_free
		position relative
		background #2687cc
		font-family "Open Sans"

		&.base-col
			height 100%

.headline-3
	display inline-block
	max-width 600px
	width 80%
	margin-left 310px
	font-weight 600
	font-size 25px
	color #FFF
	padding 50px 0
	line-height 30px

	& span
		font-size 14px
		font-weight 400
		letter-spacing 1px

	&:before
		display block
		width 162px
		height 292px
		content ''
		background url('../images/man.png') no-repeat
		position absolute
		left 60px
		bottom 0

.custom_free__locate
	display inline-block
	float right
	width 220px
	position absolute
	top 62px
	right 0

@media (max-width 1100px)
	.headline-3
		font-size 22px

	.custom_free__locate
		width auto
		right 12px
		.btn-locate
			padding 9px 16px
			font-size 14px

/*BLOCK 5*/
.section
	width 100%

	&.-price_plan
		padding-top 25px
		background #fff
		color #000
		font-family "Open Sans"
		text-align center

		.price_plan--label
			font-family 'Open Sans'
			font-size 14px
			color #4A4A4A
			line-height 30px
			font-weight 300

.price_plan__buttons
	font-size 0
	width 100%
	padding 24px 0 40px

	& input
		display none

		&:checked + label
			background #51c423
			color #fff

			&:after
				display block

	& label
		background #fff
		color #000
		border #51c423 1px solid
		padding 7px 32px
		margin-left -1px
		transition color 0.1s

		&:hover
			color #51c423

		&:after
			position absolute
			top 100%
			left 50%
			margin-left -8%
			content ''
			display none
			width 0
			height 0
			border-left 7px solid transparent
			border-right 7px solid transparent
			border-top 7px solid #51c423

.price_plan__select
	font-size 0
	width 100%
	padding 10px 0 70px

	li:first-child
		margin-left 0

	li:last-child
		margin-right 0

.select-col
	min-height 570px
	position relative
	vertical-align top
	padding-bottom 100px
	display inline-block
	width calc(100% /4 - 20px)
	-webkit-border-radius 10ox
	-moz-border-radius 10px
	border-radius 10px
	margin 0 12px
	box-shadow 0px 2px 10px 0px rgba(0,0,0,0.12)

	& p
		font-size 14px
		line-height 32px
		color #525252


	del
		color #c0c0c0
		text-decoration line-through
		line-height 32px


.select__name
	padding 24px 0 0
	font-family 'Montserrat'
	font-size 22px
	color #000

	&:after
		display block
		content ''
		width 150px
		height 1px
		margin 20px auto 0
		background #e0e0e0

.select__from
	display block
	font-size 13px
	font-weight 300
	color #c5c4c5
	line-height 30px
	margin-top 16px

.select__price
	font-family 'Montserrat'
	font-size 44px
	color #000
	padding-bottom 20px

/*BLOCK 6*/
.section
	width 100%

	&.-interesting_stories
		padding-top 35px
		background #f3f3f3
		color #000
		font-family "Open Sans"

		& h2
			text-align center

.block_stories
	padding-bottom 50px

.list_stories
	-webkit-box-sizing border-box
	-moz-box-sising border-box
	box-sizing border-box
	font-size 0
	width 100%
	padding 0 10px

.stories-col
	display inline-block
	vertical-align top
	width calc(100% /3 - 14px)
	margin 35px 10px
	background #fff
	box-shadow 0px 2px 10px 0px rgba(0,0,0,0.12)
	text-decoration none
	min-height 320px
	color #000
	transition box-shadow 0.2s linear, background 0.2s linear

	&:hover
		box-shadow 0px 2px 10px 0px rgba(0,0,0,0.22)
		background #fdfcfc

		.stor__strong
			color #e82222

	& span
		display block
		padding-top 6px

.break-wc
	margin-left 0

	&:before
		display block
		width auto
		height 202px
		content ''
		background url('../images/wc.png') no-repeat

.look-up

	&:before
		display block
		width auto
		height 202px
		content ''
		background url('../images/look.png') no-repeat

.maintenance
	margin-right 0

	&:before
		display block
		width auto
		height 202px
		content ''
		background url('../images/maintenance.png') no-repeat

.stor__strong
	font-size 18px
	padding 20px 30px
	line-height 24px
	color #000
	transition color 0.2s

	& span
		font-size 14px
		color #868686
		font-weight 300

/*BLOCK 7*/
	section
		width 100%

		&.-call_me
			background #2687cc
			font-family "Open Sans"
			text-align center
			transition background 0.25s

			&.-success
				background #51C423

			& form
				position relative
				padding 50px 0

				.form-input
					position relative
					left -10px

/*FOOTER*/
.footer
	width 100%
	background #fff
	color #9b9b9b
	font-family "Open Sans"
	letter-spacing 0.7px
	font-size 15px
	line-height 20px

.footer__list
	padding-top 80px

.footer-col
	vertical-align top
	display inline-block
	a
		font-weight 700
		transition color 0.2s

		&:hover
			color #000

.copyright
	width 25%
	padding-left 30px

.social_buttons
	padding 20px 0

	& img
		padding 0 8px

.contacts
	width calc(100% /3)
	padding-top 15px

	& table
		border-collapse separate
		border-spacing 12px
		padding-top 5px

	& span
		color #000
		font-size 22px
		line-height 25px
		padding-left 15px
		font-weight 600

.help
	width calc(100% /3)
	margin-right 0
	padding 15px 60px 0 0
	-webkit-box-sizing border-box
	-moz-box-sizing border-box
	box-sizing border-box

	& span
		color #000
		font-size 22px
		line-height 25px
		font-weight 600

	& p
		padding-top 10px
		line-height 24px

.cont_number
	color #2482c5
	text-decoration none
	display inline

.cont_mail
	color #51c423
	text-decoration none

.difiz_label
	text-align center
	padding 35px 0 80px

/*HELP PAGE. BLOCK 2 QUESTIONS*/
.section
	width 100%

	&.-questions
		width 100%
		background transparent
		font-family "Open Sans"
		font-size 16px
		color #000
		line-height 40px
		padding 190px 0 90px

		& p

			&:after
				display block
				content ''
				width 100%
				height 1px
				margin 15px auto
				background #e2e2e2

			&:last-child

				&:after
					display none

.help_list
	width 100%
	position relative
	margin -320px 0 64px
	background transparent
	color #000
	padding 0 4px 0 10px
	box-sizing border-box

.help-col
	width calc(100% /4 - 12px)
	position relative
	display inline-block
	margin 0 5px
	padding-top 20px
	background #fff
	-webkit-border-radius 10px
	-moz-border-radius 10px
	border-radius 10px
	text-align center
	font-family "Open Sans"
	overflow hidden

	a
		// position absolute
		top 0
		left 0
		width 100%
		height 100%
		text-decoration none

		&:hover
			.help__span
				background #176195

		&.-active
			.help__span
				background #51C423

	img
		display block
		margin 0 auto

.help_install
	margin-left 0

.exceptional_cases
	margin-right 0

.help__span
	display block
	margin-top 24px
	font-weight 600
	font-size 22px
	color #FFF
	line-height 22px
	padding 16px 0
	background #2482C5
	transition background 0.2s

.popular_questions, .recent_questions
	position relative
	display inline-block
	vertical-align top
	background #fff
	width calc(50% - 20px)
	box-shadow 0px 2px 10px 0px rgba(0,0,0,0.12)
	margin -40px 10px 0
	padding 30px
	-webkit-border-radius 10px
	-moz-border-radius 10px
	border-radius 10px
	box-sizing border-box
	line-height 28px

	strong
		display block
		margin-bottom -15px

	a
		display block
		font-size 16px
		color #000
		line-height 22px
		text-decoration none
		padding 26px 0
		border-bottom 1px solid #E2E2E2
		transition color 0.2s

		&:hover
			color #2482C5

		&:last-child
			border-bottom 0

.recent_questions
	width calc(50% - 25px)


.ques__span-head
	display inline-block
	font-size 20px
	padding-bottom 12px
	font-weight 600

	&:after
		display block
		content ''
		width 100%
		height 3px
		margin 5px 0 15px
		background #2482c5

/*HELP PAGE CATEGORY. BLOCK 2 DETAILS*/
.section
	width 100%

	&.-list_installation
		position relative
		padding-bottom 90px
		min-height 440px
		background transparent
