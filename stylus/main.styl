#subscribeEmail
	font-weight 700
	width 540px
	font-size 44px
	color #FFF
	background transparent !important
	line-height 44px
	margin-right 200px
	padding-right 24px
	animation blink 0.8s linear infinite

	&:focus
		border-color transparent
		animation none

		&::-webkit-input-placeholder
			text-indent -700px
		&:-moz-placeholder
			text-indent -700px
		&::-moz-placeholder
			text-indent -700px
		&:-ms-input-placeholder
			text-indent -700px

	&::-webkit-input-placeholder
		color #fff
		line-height 52px
		text-indent 0
		transition text-indent 0.25s
	&:-moz-placeholder
		color #fff
		line-height 52px
		text-indent 0
		transition text-indent 0.25s
	&::-moz-placeholder
		color #fff
		line-height 52px
		text-indent 0
		transition text-indent 0.25s
	&:-ms-input-placeholder
		color #fff
		line-height 52px
		text-indent 0
		transition text-indent 0.25s

	@keyframes blink {
		0% {
			border-right 4px transparent solid
		}
		50% {
			border-right 4px transparent solid
		}
		51% {
			border-right 4px #fff solid
		}
		100% {
			border-right 4px #fff solid
		}
	}

#subscribeEmail:-webkit-autofill {
		-webkit-box-shadow: 0 0 0px 1000px #2687cc inset;
		-webkit-text-fill-color: #fff !important;
}


