.tooltip
	position relative
	width 28px
	height 28px

	&:before
		content '?'
		cursor pointer
		position absolute
		left 0
		width 28px
		height 28px
		border 1px solid #DADADA
		border-radius 50%
		text-align center
		line-height 28px
		color #51C423

	&:hover:before
		background #51C423
		color #fff
		border-color #51C423

	span
		display none
		position absolute
		top 0
		left -40px
		transform translate(-50%, -100%)
		margin-top -18px
		background #fff
		box-shadow 0 5px 10px rgba(0,0,0,.12)
		border-radius 10px
		padding 18px 18px
		font-size 12px
		color #787878
		line-height 18px
		width 150px

		&:after
			content ''
			display block
			position absolute
			z-index 1
			bottom -7px
			left 50%
			margin-left 46px
			width 18px
			height 18px
			background #fff
			transform rotate(-45deg)
			box-shadow -5px 5px 10px rgba(0,0,0,.06)

	&:hover
		span
			display block

